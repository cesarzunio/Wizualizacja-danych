import random
#zad2
lista = [random.randrange(0, 100) for x in range(0, 11)]
lista2 = [x for x in lista if x % 2 == 0]

print(lista)
print(lista2)

#zad3
slownik = {
    "Ziemniaki" : "KG",
    "Kapusta" : "Szt",
    "Cos innego" : "Szt"
}

listaP = [key for key, value in slownik.items() if value == "Szt"]
print(listaP)

#zad4
def czy_pros(a, b, c):
    return a ** 2 + b ** 2 == c ** 2


print(czy_pros(3, 4, 5))


